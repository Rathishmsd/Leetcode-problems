bool isValid(char* s) {

    char stack[10000];
    int top = -1;

    for (int i = 0; s[i] != '\0'; i++) 
    {
        char ch = s[i];

        if (ch == '(' || ch == '{' || ch == '[') 
        {
            stack[++top] = ch;
        } 
        else 
        {
            if (top == -1)
                return false;

            char open = stack[top--];

            if (!((open == '(' && ch == ')') ||
                  (open == '{' && ch == '}') ||
                  (open == '[' && ch == ']')))
                return false;
        }
    }
    if(top==-1)
    {
       return true;
    }
    else
    return false;
}
